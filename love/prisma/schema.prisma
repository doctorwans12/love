generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Attempt {
  id              String   @id @default(cuid())
  createdAt       DateTime @default(now())
  answers         Json
  traits          Json
  archetype       String
  stripeSessionId String?  @unique
  paid            Boolean  @default(false)
  adviceText      String?
}
